{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "resolveJsonModule": true,
    "paths": {
      "*": ["./*"]
    }
  },
  "include": ["src/**/*", "webpack.config.ts"],
  "exclude": ["node_modules"]
}
